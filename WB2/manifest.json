{
    "manifest_version": 2,
    "name": "WB2",
    "version": "1.0",
    "description": "Description",
    "permissions": [
      "activeTab",
      "storage",
      "clipboardRead",
      "webNavigation",
      "webRequest",
      "webRequestBlocking",
      "http://*/*",
      "https://*/*"
    ],
    "browser_action": {
      "default_popup": "h.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["j.js"]
      }
    ]
  }
  